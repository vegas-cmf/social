        "facebook/php-sdk-v4": "4.0.*",
        "themattharris/tmhoauth": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vegas\\Social\\": "src/",
            "Vegas\\Tests\\": "tests/"
        }
